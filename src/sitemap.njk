---
layout: false
permalink: /sitemap.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <sitemap><loc>{{ '/sitemaps/content.xml' | absoluteUrl(site.url) }}</loc></sitemap>
  <sitemap><loc>{{ '/sitemaps/hubs.xml' | absoluteUrl(site.url) }}</loc></sitemap>
  {% for l in site.languages %}
  <sitemap><loc>{{ ('/sitemaps/' + l.code + '.xml') | absoluteUrl(site.url) }}</loc></sitemap>
  {% endfor %}
</sitemapindex>
