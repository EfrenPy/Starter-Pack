<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
<meta http-equiv="Content-Security-Policy" content="{{ site.csp | safe }}">
<meta name="view-transition" content="same-origin">
{% if description %}
<meta name="description" content="{{ description }}">
{% endif %}
{% if og %}
<meta property="og:title" content="{{ og.title or title }}">
<meta property="og:description" content="{{ og.description or description }}">
{% else %}
<meta property="og:title" content="{{ title }}">
<meta property="og:description" content="{{ description }}">
{% endif %}
<meta property="og:type" content="website">
<meta property="og:url" content="{{ site.url }}{{ page.url }}">
<meta property="og:locale" content="{{ locale }}">
<meta property="og:image" content="{{ site.url }}{{ site.ogImage }}">
<meta property="og:image:width" content="{{ site.ogImageWidth }}">
<meta property="og:image:height" content="{{ site.ogImageHeight }}">
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:image" content="{{ site.url }}{{ site.ogImage }}">
<meta name="twitter:title" content="{{ og.title or title }}">
<meta name="twitter:description" content="{{ og.description or description }}">
<link rel="canonical" href="{{ site.url }}{{ page.url }}">
{% for l in site.languages %}
<link rel="alternate" hreflang="{{ l.code }}" href="{{ site.url }}{{ page.url | toLang(lang, l.code) }}">
{% endfor %}
<link rel="alternate" hreflang="x-default" href="{{ site.url }}/">
<link rel="icon" type="image/png" sizes="32x32" href="/images/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="16x16" href="/images/favicon-16x16.png">
<link rel="apple-touch-icon" sizes="180x180" href="/images/apple-touch-icon.png">
<link rel="manifest" href="/site.webmanifest">
{% if noindex %}
<meta name="robots" content="noindex">
{% endif %}
<title>{{ title }}</title>
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=DM+Sans:opsz,wght@9..40,400;9..40,500;9..40,600;9..40,700&display=swap" rel="stylesheet">
<link rel="preload" href="/css/styles.css" as="style">
<link rel="stylesheet" href="/css/styles.css">
<script>{{ site.themeScript | safe }}</script>
{% block schema %}{% endblock %}
